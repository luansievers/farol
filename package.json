{
  "name": "farol",
  "version": "1.0.0",
  "description": "Radar de Contratos - Analise automatica de contratos publicos",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "pnpm --filter @farol/api api:dev",
    "dev:web": "pnpm --filter @farol/web dev",
    "dev:all": "pnpm --parallel --filter './packages/*' dev",
    "build": "pnpm --filter @farol/shared build && pnpm --filter './packages/*' build",
    "build:shared": "pnpm --filter @farol/shared build",
    "build:api": "pnpm --filter @farol/api build",
    "build:web": "pnpm --filter @farol/web build",
    "test": "pnpm --filter './packages/*' test",
    "lint": "pnpm --filter './packages/*' lint",
    "typecheck": "pnpm --filter './packages/*' typecheck",
    "api": "pnpm --filter @farol/api api",
    "api:dev": "pnpm --filter @farol/api api:dev",
    "db:generate": "pnpm --filter @farol/api db:generate",
    "db:migrate": "pnpm --filter @farol/api db:migrate",
    "db:migrate:deploy": "pnpm --filter @farol/api db:migrate:deploy",
    "db:push": "pnpm --filter @farol/api db:push",
    "db:seed": "pnpm --filter @farol/api db:seed",
    "db:studio": "pnpm --filter @farol/api db:studio",
    "db:reset": "pnpm --filter @farol/api db:reset",
    "crawler": "pnpm --filter @farol/api crawler",
    "crawler:week": "pnpm --filter @farol/api crawler:week",
    "crawler:month": "pnpm --filter @farol/api crawler:month",
    "detail": "pnpm --filter @farol/api detail",
    "detail:batch": "pnpm --filter @farol/api detail:batch",
    "detail:stats": "pnpm --filter @farol/api detail:stats",
    "detail:reset": "pnpm --filter @farol/api detail:reset",
    "parser": "pnpm --filter @farol/api parser",
    "parser:batch": "pnpm --filter @farol/api parser:batch",
    "parser:stats": "pnpm --filter @farol/api parser:stats",
    "parser:reset": "pnpm --filter @farol/api parser:reset",
    "summary": "pnpm --filter @farol/api summary",
    "summary:batch": "pnpm --filter @farol/api summary:batch",
    "summary:stats": "pnpm --filter @farol/api summary:stats",
    "summary:reset": "pnpm --filter @farol/api summary:reset",
    "summary:regen": "pnpm --filter @farol/api summary:regen",
    "classify": "pnpm --filter @farol/api classify",
    "classify:batch": "pnpm --filter @farol/api classify:batch",
    "classify:stats": "pnpm --filter @farol/api classify:stats",
    "classify:reset": "pnpm --filter @farol/api classify:reset",
    "classify:set": "pnpm --filter @farol/api classify:set",
    "classify:reclassify": "pnpm --filter @farol/api classify:reclassify",
    "anomaly": "pnpm --filter @farol/api anomaly",
    "anomaly:batch": "pnpm --filter @farol/api anomaly:batch",
    "anomaly:stats": "pnpm --filter @farol/api anomaly:stats",
    "anomaly:reset": "pnpm --filter @farol/api anomaly:reset",
    "anomaly:reset:all": "pnpm --filter @farol/api anomaly:reset:all",
    "anomaly:recalculate": "pnpm --filter @farol/api anomaly:recalculate",
    "anomaly:single": "pnpm --filter @farol/api anomaly:single",
    "auto-update": "pnpm --filter @farol/api auto-update",
    "auto-update:start": "pnpm --filter @farol/api auto-update:start",
    "auto-update:stats": "pnpm --filter @farol/api auto-update:stats"
  },
  "keywords": [
    "contratos",
    "transparencia",
    "sp"
  ],
  "author": "",
  "license": "ISC"
}
